<section class="banner">
  <div class="banner-text container">
    <h1>C.L.A.S.S.E. CONSEIL</h1>
    <h2>Corine Pesier</h2>
    <h3>Psychologue, Consultante et Formatrice R.H. indépendante</h3>
    <a class="btn-flat-outline" href="/pratiques_professionnelles.html">En savoir plus ></a>
  </div>
  <div class='icon-scroll'>
    <div />
</section>
<section class="sub-banner section container">
  <p class="mini-title">Bienvenue</p>
  <div class="quote container">
    <div class="quote-wrapper">
      <div class="quote-text">
        <blockquote>
          <p>Développer les compétences, c'est créer de la valeur ajoutée, augmenter l'efficience, participer au
            développement personnel et susciter la motivation sous jacente</p>
        </blockquote>
      </div>
    </div>
  </div>
  <div class="introduction">
    <div class="wrapper-left">
      <%= image_tag "https://res.cloudinary.com/dqszntd5j/image/upload/v1592933200/logo.png", alt: "C.L.A.S.S.E. Conseil - Logo", id:"intro-logo" %>
    </div>
    <div class="wrapper-right">
      <h2 class="section-title" id="sub-banner-title">28 années d'expertise dans:</h2>
      <div class="xp">
        <h3><i class="fas fa-check"></i>L’accompagnement individuel et collectif</h3>
        <h3><i class="fas fa-check"></i>Le bilan de compétences</h3>
        <h3><i class="fas fa-check"></i>La formation</h3>
      </div>
    </div>
  </div>
</section>
<div class="separator-gradient"></div>
