      <%= link_to 'En savoir plus <i class="fas fa-arrow-right"></i>', "/formations.html", class: "card-links" %>
    </div>
    <% end %>
  </div>
</section>
<div class="separator-gradient"></div>
<section class="reinsurance section container">
  <p class="mini-title" id="mini-title-service">Travaillons ensemble</p>
